{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "Linux",
            "command": "/usr/bin/g++",
            "args": [
                "-g",
                "-std=c++20",
                "${workspaceFolder}/src/symmetric/AES/AESKeySchedule.cpp",
                "${workspaceFolder}/src/symmetric/AES/AES.cpp",
                "${workspaceFolder}/src/symmetric/AES/AESType.cpp",
                "${workspaceFolder}/src/symmetric/AES/State.cpp",
                "${workspaceFolder}/src/symmetric/TDES/TDES.cpp",
                "${workspaceFolder}/src/symmetric/DES/DESMatrixUtils.cpp",
                "${workspaceFolder}/src/symmetric/DES/DES.cpp",
                "${workspaceFolder}/src/CypherError.cpp",
                "${workspaceFolder}/src/Main.cpp",
                "-o",
                "${workspaceFolder}/out/linux/Main"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },

        {
            "type": "cppbuild",
            "label": "Windows",
            "command": "x86_64-w64-mingw32-g++",
            "args": [
                "-g",
                "-std=c++20",
                "${workspaceFolder}/src/symmetric/DES/DESMatrixUtils.cpp",
                "${workspaceFolder}/src/symmetric/DES/DES.cpp",
                "${workspaceFolder}/src/CypherError.cpp",
                "${workspaceFolder}/src/Main.cpp",
                "-o",
                "${workspaceFolder}/out/windows/Main.exe"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}